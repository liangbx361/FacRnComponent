<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- 字体主颜色：黑色-->
    <color name="me_main_text_color">#131313</color>
    <!-- 字体次颜色：灰色-->
    <color name="me_second_text_color">#828282</color>

    <!-- 界面背景颜色 -->
